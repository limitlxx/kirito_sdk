{
  "name": "@kirito/sdk",
  "version": "1.0.0",
  "description": "Privacy-first toolkit for creating, minting, and managing yield-generating NFTs on Starknet",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run build:contracts",
    "build:contracts": "cd contracts && scarb build",
    "dev": "tsc --watch",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --runInBand",
    "test:properties": "jest tests/properties --runInBand",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "docs": "typedoc --out docs src/sdk/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "nft",
    "privacy",
    "starknet",
    "zero-knowledge",
    "yield",
    "cairo",
    "defi",
    "bitcoin",
    "btc",
    "wbtc",
    "mystery-box",
    "governance",
    "auction",
    "wallet",
    "account-abstraction"
  ],
  "author": "Kirito SDK Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kirito-sdk/kirito-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/kirito-sdk/kirito-sdk/issues"
  },
  "homepage": "https://github.com/kirito-sdk/kirito-sdk#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@types/gifencoder": "^2.0.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "fast-check": "^3.15.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@atomiqlabs/chain-starknet": "latest",
    "@atomiqlabs/sdk": "^8.1.8",
    "@atomiqlabs/storage-sqlite": "latest",
    "@fatsolutions/tongo-sdk": "^1.3.1",
    "@noir-lang/backend_barretenberg": "^0.36.0",
    "@noir-lang/noir_js": "^1.0.0-beta.18",
    "ethers": "^6.0.0",
    "garaga": "^1.0.1",
    "gifencoder": "^2.0.1",
    "ipfs-http-client": "^60.0.0",
    "sats-connect": "4.2.x",
    "sharp": "^0.33.5",
    "starknet": "^8.5.4"
  },
  "optionalDependencies": {
    "canvas": "^2.11.2"
  },
  "files": [
    "dist/**/*",
    "contracts/target/**/*",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
